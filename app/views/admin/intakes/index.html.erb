<h1>Intakes</h1>

<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">source</th>
      <th colspan="3" scope="col">actions</th>
    </tr>
  </thead>

  <tbody>
    <% @intakes.each_with_index do |intake, idx| %>
      <tr>
        <td scope="row"><%= idx %>
        <td><%= intake.name %> (<%= intake.email %>)</td>
        <td><%= link_to 'Show', admin_intake_path(intake) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

