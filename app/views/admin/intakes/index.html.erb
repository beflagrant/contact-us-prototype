<p id="notice"><%= notice %></p>

<h1>Intakes</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @intakes.each do |intake| %>
      <tr>
        <td>from: <%= intake.email %></td>
        <td><%= link_to 'Show', admin_intake_path(intake) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

